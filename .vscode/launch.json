{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Attatch to renderer",
      "type": "node",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "port": 2992
    },
    {
      "name": "Atatch to server",
      "type": "node",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ],
      "port": 9229
    }
  ]
}
