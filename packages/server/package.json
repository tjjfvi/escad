{
  "name": "@escad/server",
  "version": "0.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "build": "npm run clean; tsc",
    "hash": "find src/ -type f -print0 | sort -z | xargs -0 openssl dgst -sha256 | openssl dgst -sha256 > .hash",
    "changed": "touch .hash; mv .hash old.hash; npm run hash; cmp old.hash .hash >/dev/null 2>&1",
    "quick-build": "npm run changed && echo 'Skipping build' || npm run force-build; rm old.hash",
    "prepack": "npm run build",
    "open-errors": "tsc | grep -p '^\\S' | sed -e 's/(.*//' | sort | uniq | xargs code"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "browserify": "^16.5.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "flatted": "^3.0.2",
    "fs-extra": "^9.0.0",
    "node-watch": "^0.6.4",
    "stylus": "^0.54.7",
    "ts-node": "^8.10.2",
    "tsee": "^1.3.0",
    "uuid": "^8.1.0",
    "watchify": "^3.11.1",
    "@escad/client": "^0.0.0",
    "@escad/core": "^0.0.0",
    "@escad/renderer": "^0.0.0",
    "@escad/server-renderer-messages": "^0.0.0",
    "@escad/server-client-messages": "^0.0.0",
    "@escad/register-client-plugin": "^0.0.0",
    "@escad/client-bundler": "^0.0.0"
  },
  "devDependencies": {
    "@types/browserify": "^12.0.36",
    "@types/express": "^4.17.6",
    "@types/express-ws": "^3.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.0.13",
    "@types/stylus": "^0.48.32",
    "@types/uuid": "^8.0.0",
    "@types/watchify": "^3.11.0",
    "typescript": "^4.1.0-dev.20201015"
  },
  "publishConfig": {
    "access": "public"
  }
}