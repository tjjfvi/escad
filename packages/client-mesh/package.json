{
  "name": "@escad/client-mesh",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "stylus": "stylus",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "force-build": "npm run clean; tsc",
    "hash": "find src/ -type f -print0 | sort -z | xargs -0 openssl dgst -sha256 | openssl dgst -sha256 > .hash",
    "changed": "touch .hash; mv .hash old.hash; npm run hash; cmp old.hash .hash >/dev/null 2>&1",
    "build": "npm run changed && echo 'Skipping build' || npm run force-build; rm old.hash",
    "prepack": "npm run force-build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@escad/client": "^0.0.0",
    "@escad/client-3d-viewer": "^0.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rhobo": "^0.3.2",
    "three": "^0.112.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "typescript": "^3.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}