    "@escad/client-bundler": "^0.0.0",
    "@escad/messages": "^0.0.0",
    "commander": "^7.0.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "node-watch": "^0.7.1"
  },
  "devDependencies": {
    "@types/express-ws": "^3.0.0",
    "@types/express": "^4.17.11",
    "typescript": "^4.1.3"
